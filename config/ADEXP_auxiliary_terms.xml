<!-- Structure of this XML -->
<!-- <AuxilliaryTerm name="" regexp="" errmsg="" multi=""></AuxilliaryTerm> -->

<!-- 
ALPHA: [A-Z]
DIGIT: [0-9]
ALPHANUM: [A-Z0-9]
SPACE: [ ]
HYPHEN: [-]
FEF: [\r\n]
SEP: [ \r\n]+
SPECIAL: [ \(\)\?:\.,'=+\/]
CHARACTER: [A-Z0-9 \(\)\?:\.,'=+\/\r\n-]
LIM_CHAR: [A-Z0-9 \(\)\?:\.,'=+\/\r\n]
START-OF-FIELD: ^-
century: [0-9]{2}
year: [0-9]{2}
month: (0|1)[0-9]
day: (0|1|2|3)[0-9] 
 -->
<AuxTerms>
    <AuxilliaryTerm id="adexpmsg" regexp="[A-Z0-9 \(\)\?:\.,'=+/\r\n-]*" errmsg="" multi=""/>
    <AuxilliaryTerm id="aidequipment" regexp="^(N|S)?(A|B|C|D|E1|E2|E3|F|G|H|I|J1|J2|J3|J4|J5|J6|J7|K|L|M1|M2|M3|O|P1|P2|P3|P4|P5|P6|P7|P8|P9|R|S|T|U|V|W|X|Y|Z)+" errmsg="" multi=""/>
